#!/bin/ bash
alias mc.sleepscreen='/System/Library/CoreServices/"Menu Extras"/User.menu/Contents/Resources/CGSession -suspend'
alias mc.lockscreen='/System/Library/Frameworks/ScreenSaver.framework/Resources/ScreenSaverEngine.app/Contents/MacOS/ScreenSaverEngine'
alias mc.edit='open -a MacVim.app $*'
alias mc.notesmv='open -a MacVim.app  $HOME/.notes_flist --args -n'
alias mc.tags='cat ~/.tagfiles | grep Tag= | grep -v ".tagfiles" | sed -e"s/Tag=//"'
#alias mc.worknotes='(cd ~/workspace/worknotes.links/ && fzf --preview "bat --color=always {}" --prompt "Search Code: " --border --layout=reverse --bind "enter:execute(nvim {})")'
alias mc.wn='find ~/workspace/worknotes.links | .fzf'
#alias mc.ps='ps -eo pid,ppid,args | grep "[v]im"' This will exclude the grep vim command from filter.
alias mc.ps='ps -eo pid,ppid,args'

alias mc.eclipse="$HOME/Downloads/eclipse/Eclipse.app/Contents/MacOS/eclipse"
alias mc.as='open -a "Android Studio.app"'
alias mc.outlook='open -a "Microsoft Outlook.app"'
alias mc.nano='/opt/homebrew/bin/nano'
alias mc.vsc='open -a /Applications/Visual\ Studio\ Code.app'
alias mc.pgadmin='open -a "pgAdmin 4.app"'

export MAC_DEF_BROWSER='Safari'
alias mc.setBrow=' function f() { if [[ "$1" == "c" ]]; then MAC_DEF_BROWSER="Google Chrome"; else MAC_DEF_BROWSER="Safari"; fi; }; f'
alias mc.jr='function f() { open -a "$MAC_DEF_BROWSER" "https://$JIRA_SERVER_NAME/browse/$1"; }; f'
alias mc.et='function f() { open -a "$MAC_DEF_BROWSER" "https://${ETRACK_SERVER_NAME}/Etrack/readonly_inc.php?incident=${1}"; }; f'
alias mc.eq='function f() { open -a "$MAC_DEF_BROWSER" "https://${ETRACK_SERVER_NAME}/Etrack/readonly_inc.php?querychosen=${1}"; }; f'
alias mc.sc='function f() { open -a "$MAC_DEF_BROWSER" https://${SUPPORT_CASE_SERVER_NAME}/Cases/case/${1};}; f'
alias mc.brow='function f() { open -a "$MAC_DEF_BROWSER" $1; }; f'

alias ol.ls='ollama list'
alias ol.lsm='ollama list | cut -d":" -f1'
alias ol.rfzf='ollama run $(ollama list | tail -n +2 | awk '"'"'{print $1}'"'"' | fzf)'
alias ol.status='curl -sSf http://localhost:11434/api/version && echo " ollama is running" || echo "ollama Not running"'
alias ol.stop='brew services stop ollama'
alias ol.pid='launchctl list | grep ollama'
alias ol.start='nohup ollama serve > $op/ollama.log 2>&1 &'
alias ol.log='tmux display-popup -E -w 80% -h 50% '"'"'tail -f $op/ollama.log; read -n 1 -s -r -p "Press any key"'"'"''
alias ol.rpc.2='function af.ollama() { ollama run ${3:-mistral} <<< "$(cat ${1:-ollama}.pmpt; printf '"'"'\n'"'"'; cat ${1:-ollama}.sh)"; }; af.ollama'
alias ol.rpc.3='function af.ollama() { ollama run ${3:-mistral} <<< "$(cat ${2:-ollama.pmpt}; printf '"'"'\n'"'"'; cat ${1:-ollama.sh})"; }; af.ollama'

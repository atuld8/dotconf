alias x.unc2sftp.evd='function af.unc2http() { echo "$@" | sed -e "s/^\\\\/:\/\//g" | sed -E "s/\.comm(.*)/\-sftp.comm\1/g" | sed -e "s/\\\\/\//g" | sed -e "s/$/\//g" | xargs -I {} sh -c "echo \"sftp{}\"; echo \"scp{}\""; }; af.unc2http'

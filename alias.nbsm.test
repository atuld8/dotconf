alias t.nbsm.env='mkdir -p /usr/openv/nbsm; mkdir /usr/openv/nbsm/nbserverupdate; chmod 755 /usr/openv/nbsm/nbserverupdate'

alias cd.nbsm='af.cd /usr/openv/nbsm'
alias cd.nbsmpy='af.cd /usr/openv/nbsm/nbserverupdate'

alias t.nbrn.pr='function at.t.nbrn.pr() { nbserver_update -operation rename -current_name $(bpgetconfig | grep SERVER | head -1 | cut -d " " -f3) -new_name $1 -pre-report; }; af.t.nbrn.pr'

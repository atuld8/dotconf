alias t.nbsm.md='mkdir -p /usr/openv/nbsm; mkdir -p /usr/openv/nbsm/nbserverupdate; chmod 755 /usr/openv/nbsm/nbserverupdate'

alias cd.nbsm='af.cd /usr/openv/nbsm'
alias cd.nbsmpy='af.cd /usr/openv/nbsm/nbserverupdate'

alias t.nbsup.prr='function af.t.nbrn.pr() { cd.nbsmpy; if [[ -z "$1"  && -z $NBSM_NEW_NAME ]]; then echo "Action: Pass arg or set NBSM_NEW_NAME"; return 1; fi;  nbserver_update -operation rename -current_name $(bpgetconfig | grep SERVER | head -1 | cut -d " " -f3) -new_name ${1:-$NBSM_NEW_NAME} -pre_report; cd -; }; af.t.nbrn.pr'
alias t.nbsup.r='function af.t.nbrn.pr() { cd.nbsmpy; if [[ -z "$1"  && -z $NBSM_NEW_NAME ]]; then echo "Action: Pass arg or set NBSM_NEW_NAME"; return 1; fi;  nbserver_update -operation rename -current_name $(bpgetconfig | grep SERVER | head -1 | cut -d " " -f3) -new_name ${1:-$NBSM_NEW_NAME}; cd -; }; af.t.nbrn.pr'
alias t.nbsup.por='function af.t.nbrn.pr() { cd.nbsmpy; if [[ -z "$1"  && -z $NBSM_NEW_NAME ]]; then echo "Action: Pass arg or set NBSM_NEW_NAME"; return 1; fi;  nbserver_update -operation rename -current_name $(bpgetconfig | grep SERVER | head -1 | cut -d " " -f3) -new_name ${1:-$NBSM_NEW_NAME} -post_report; cd -; }; af.t.nbrn.pr'
